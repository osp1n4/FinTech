{
  "name": "fraud-detection-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-End tests for Fraud Detection Engine using Playwright",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:trace": "playwright show-trace",
    "test:chromium": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:admin": "playwright test tests/admin-dashboard.spec.ts",
    "test:user": "playwright test tests/user-app.spec.ts",
    "test:api": "playwright test tests/api-integration.spec.ts"
  },
  "keywords": [
    "playwright",
    "e2e",
    "testing",
    "fraud-detection",
    "screenplay",
    "page-object-model"
  ],
  "author": "María Paula Gutiérrez",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@types/node": "^20.11.0",
    "dotenv": "^16.4.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {}
}
